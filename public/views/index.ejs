                                                        <% if (!tipeeeEnabled) { %>
                                                            <div class="step">
                                                                <ul class="nav nav-tabs nav-justified" role="tablist">
                                                                    <li role="step" class="active">
                                                                        <a href="#step1" id="step1-tab" role="tab" data-toggle="tab" aria-controls="home" aria-expanded="true" data-ytta-id="-">
